lexer(HL::Lexers::Properties):

name: "Properties"
aliases: ["properties"]
extensions: [".properties"]
mimetypes: ["text/x-java-properties"]
start: #root
flags: 0

lex(root):
  Regexp new("\\s+") is(text)
  Regexp new("(?:[;#]|//).*$") is(comment)
  Regexp new("(.*?)([ \\t]*)([=:])([ \\t]*)(.*(?:(?<=\\\\)\\n.*)*)") is(by-groups(name.attribute, text, operator, text, literal.string))

