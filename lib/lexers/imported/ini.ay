lexer(HL::Lexers::Ini):

name: "INI"
aliases: ["ini", "cfg"]
extensions: [".ini", ".cfg"]
mimetypes: ["text/x-ini"]
start: #root
flags: 0

lex(root):
  Regexp new("\\s+") is(text)
  Regexp new("[;#].*?$") is(comment)
  Regexp new("\\[.*?\\]$") is(keyword)
  Regexp new("(.*?)([ \\t]*)(=)([ \\t]*)(.*(?:\\n[ \\t].+)*)") is(by-groups(name.attribute, text, operator, text, literal.string))

