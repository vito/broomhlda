$path << File expand-path("../", _FILE)

import("macros")
import("token")
import("lexers")
import("state")
import("formatters/html")
import("lexers/atomo")

atomo = HL::Lexers::Atomo new(
  "nil {- hello {- goodbye -} -}
true 1.0e10 a-b-c

macro operator

for-macro for-macros for-macro!

abcd + efg

\"hello!\\DEL\"

#foo
#abc
foo[b\\a\\]r]
"
)
res = atomo run
puts(HL::Formatters::Html new(res, false) format)
